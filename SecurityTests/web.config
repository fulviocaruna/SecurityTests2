<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<security>
			<requestFiltering removeServerHeader="true" />
		</security>
		<httpProtocol>
			<customHeaders>
				<remove name="X-Powered-By" />
				<add name="Referrer-Policy" value="no-referrer" />
			</customHeaders>
		</httpProtocol>
	</system.webServer>
</configuration>